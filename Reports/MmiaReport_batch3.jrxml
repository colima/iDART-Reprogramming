	<parameter name="startDate" class="java.util.Date"/>
	<parameter name="endDate" class="java.util.Date"/>
	<queryString>
		<![CDATA[select
ini.iniadult,
ini.inichild,
man.manadult,
man.manchild,
alt.altadult,
alt.altchild,
ptv.ptv,
ppe.ppe,
transf.transfadult,
transf.transfchild
from
(
select
sum(case when current_timestamp - pat.dateofbirth > interval '15 years' then 1 else 0 end) as iniadult,
sum(case when current_timestamp - pat.dateofbirth <= interval '15 years' then 1 else 0 end) as inichild
from patient pat join prescription pres on pat.id=pres.patient join package pa on pres.id=pa.prescription
join sector se on pres.sectorid=se.sectorid
where pres.reasonforupdate='Novo' or pres.reasonforupdate='Reinício' and se.sectorname <> 'SMI/PTV' and se.sectorname <> 'PPE'
and pa.pickupdate between $P{startDate}::timestamp and $P{endDate}::timestamp
) as ini,
(
select
sum(case when current_timestamp - pat.dateofbirth > interval '15 years' then 1 else 0 end) as manadult,
sum(case when current_timestamp - pat.dateofbirth <= interval '15 years' then 1 else 0 end) as manchild
from patient pat join prescription pres on pat.id=pres.patient join package pa on pres.id=pa.prescription
join sector se on pres.sectorid=se.sectorid
where pres.reasonforupdate='Manutenção' and se.sectorname <> 'SMI/PTV' and se.sectorname <> 'PPE'
and pa.pickupdate between $P{startDate}::timestamp and $P{endDate}::timestamp
) as man,
(
select
sum(case when current_timestamp - pat.dateofbirth > interval '15 years' then 1 else 0 end) as altadult,
sum(case when current_timestamp - pat.dateofbirth <= interval '15 years' then 1 else 0 end) as altchild
from patient pat join prescription pres on pat.id=pres.patient join package pa on pres.id=pa.prescription
join sector se on pres.sectorid=se.sectorid
where pres.reasonforupdate='Alteração' and se.sectorname <> 'SMI/PTV' and se.sectorname <> 'PPE'
and pa.pickupdate between $P{startDate}::timestamp and $P{endDate}::timestamp
) as alt,
(
select count(pat.dateofbirth) as ptv from patient pat join prescription pres on pat.id=pres.patient join package pa on pres.id=pa.prescription
join sector se on pres.sectorid=se.sectorid
where se.sectorname = 'SMI/PTV'
and pa.pickupdate between $P{startDate}::timestamp and $P{endDate}::timestamp
) as ptv,
(
select count(pat.dateofbirth) as ppe from patient pat join prescription pres on pat.id=pres.patient join package pa on pres.id=pa.prescription
join sector se on pres.sectorid=se.sectorid
where se.sectorname = 'PPE'
and pa.pickupdate between $P{startDate}::timestamp and $P{endDate}::timestamp
) as ppe,

(
select
sum(case when current_timestamp - pat.dateofbirth > interval '15 years' then 1 else 0 end) as transfadult,
sum(case when current_timestamp - pat.dateofbirth <= interval '15 years' then 1 else 0 end) as transfchild
from patient pat join prescription pres on pat.id=pres.patient join package pa on pres.id=pa.prescription
join sector se on pres.sectorid=se.sectorid
where pres.reasonforupdate='Transferido' and se.sectorname <> 'SMI/PTV' and se.sectorname <> 'PPE'
and pa.pickupdate between $P{startDate}::timestamp and $P{endDate}::timestamp
) as transf]]>
	</queryString>
