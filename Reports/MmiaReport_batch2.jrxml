	<queryString>
		<![CDATA[select rt.regimeesquema as name,a.num as quantity from regimeterapeutico rt
left outer join
(select p.regimeid,count(p.id) as num from prescription p join package pa on p.id=pa.prescription
where pa.pickupdate between $P{startDate}::timestamp and $P{endDate}::timestamp
group by p.regimeid
) as a on a.regimeid=rt.regimeid where rt.adult=true and a.num!=0]]>
	</queryString>
